<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/inicioFragment">
    <fragment
        android:id="@+id/inicioFragment"
        android:name="com.example.barmanagerapp.InicioFragment"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" >
        <action
            android:id="@+id/action_inicioFragment_to_iniciar_Sesion_Fragment"
            app:destination="@id/iniciar_Sesion_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/iniciar_Sesion_Fragment"
        android:name="com.example.barmanagerapp.IniciarSesionFragment"
        android:label="fragment_iniciar__sesion_"
        tools:layout="@layout/fragment_iniciar__sesion_" >
        <action
            android:id="@+id/action_iniciar_Sesion_Fragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.barmanagerapp.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.barmanagerapp.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" />
    <fragment
        android:id="@+id/spots"
        android:name="com.example.barmanagerapp.SpotsFragment"
        android:label="fragment_spots"
        tools:layout="@layout/fragment_spots" />
</navigation>